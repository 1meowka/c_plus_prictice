#include <SPI.h>
#include <WiFi.h>

char ssid[] = "Имя вашей сети.";  
char pass[] = "Пароль вашей сети.";
int status = WL_IDLE_STATUS; // для статуса подключения

WIFIServer server(80);

void setup() {
  Serial.begin(9600);	//"92.246.214.18"

  while(!Serial) {
    ;
  }

  if (WiFi.status() == WL_NO_SHIELD) {
    Serial.println("WiFi не найден.");

    while (true);
  }

  while (status != WL_CONNECTED) {
    Serial.print("Подключение к:");
    Serial.print(ssid);

    status = WiFi.begin(ssid, pass);

    delay(10000);
  }
  
  Serial.print("Вы подключены к сети.");
  printCurrentNet();
  printWifiData();  
}

void loop() {
  delay(10000);
  
  printCurrentNet();
}

void printWiFiData() {
  IPAdress ip = WiFi.localIP(); //узнаем и отправляем свой айпи
  
  Serial.print("Локальный IP адрес: ");
  Serial.println(ip);
} 

void printCurrentNet() {   // функция отправки имени сети к которой подкоючены
  Serial.print("SSID: ");
  Serial.println(WiFi.SSID());
}









