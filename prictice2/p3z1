#include <SPI.h>
#include <WiFi.h>

void setup() {
  // put your setup code here, to run once:     
  Serial.begin(9600);	//"92.246.214.18"

  while(!Serial) {
    ;
  }

  if (WiFi.status() == WL_NO_SHIELD) {
    Serial.println("WiFi ne nayden");

    while (true);
  }

  while (status != WL_CONNECTED) {
    Serial.print("Podkluchenie k:");
    Serial.print(ssid);

    status = WiFi.begin(ssid, pass);

    delay(10000);
  }
  
  Serial.print("Vi podklucheni k seti");
  printCurrentNet();
  printWifiData();
}
 
