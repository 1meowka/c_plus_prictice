void setup() {
  // put your setup code here, to run once:
  pinMode(GP0, OUTPUT);
  pinMode(GP1, INPUT);

}

void loop() {
  // put your main code here, to run repeatedly:
  if (digitalRead(GP1) == HIGH)
  {
    digitalWrite(GP0); //zazhigaem svetodiod
  }
  
  if (digitalRead(GP1) == LOW)
  {
    digitalWrite(GP0, LOW);
  }

}
